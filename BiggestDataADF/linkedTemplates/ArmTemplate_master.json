{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "BiggestDataADF"
		},
		"PSTSSIS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'PSTSSIS'"
		},
		"SSISBlob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SSISBlob'"
		},
		"RestApiSource_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.mocki.io/v1/b043df5a"
		},
		"RestService2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.exchangeratesapi.io/latest?base=AUD"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "BiggestDataADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"PSTSSIS_connectionString": {
						"value": "[parameters('PSTSSIS_connectionString')]"
					},
					"SSISBlob_connectionString": {
						"value": "[parameters('SSISBlob_connectionString')]"
					},
					"RestApiSource_properties_typeProperties_url": {
						"value": "[parameters('RestApiSource_properties_typeProperties_url')]"
					},
					"RestService2_properties_typeProperties_url": {
						"value": "[parameters('RestService2_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}